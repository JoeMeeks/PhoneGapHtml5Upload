<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets"
	xmlns:gap="http://phonegap.com/ns/1.0"
	id="com.joemeeks.html5upload"
	version="0.1.0">
	<name>HTML5 Upload</name>
	<description>Upload files in a PhoneGap app with an HTML5 file input and jQuery AJAX multi-part form post</description>
	<author href="http://joemeeks.net" email="joemeeks@joemeeks.net">Joe Meeks</author>

  <icon src="img/icons/html5-16.png" width="16" height="16"  gap:platform="ios" />
  <icon src="img/icons/html5-24.png" width="24" height="24"  gap:platform="ios" />
  <icon src="img/icons/html5-57.png" width="57" height="57"  gap:platform="ios" />
  <icon src="img/icons/html5-72.png" width="72" height="72" gap:platform="ios" gap:role="default" />
  <icon src="img/icons/html5-96.png" width="96" height="96" gap:platform="ios" />
  <icon src="img/icons/html5-114.png" width="114" height="114" gap:platform="ios" />
  <icon src="img/icons/html5-144.png" width="144" height="144" gap:platform="ios" />
  <icon src="img/icons/html5-512.png" width="512" height="512" gap:platform="ios" />

  <feature name="http://api.phonegap.com/1.0/device" />
  <feature name="http://api.phonegap.com/1.0/file" />

  <preference name="phonegap-version" value="2.5.0" />
  <preference name="orientation" value="portrait" />
  <preference name="target-device" value="universal" />
  <preference name="fullscreen" value="true" />
  <preference name="webviewbounce" value="false" />
  <preference name="prerendered-icon" value="false" />
  <preference name="detect-data-types" value="true" />
  <preference name="exit-on-suspend" value="true" />
  <preference name="stay-in-webview" value="false" />
</widget>